	<flow name="getAccounts" doc:id="be3714d2-00dd-42ff-b5ca-14cca4bfe6b9" >
		<http:listener doc:name="Listener" doc:id="74d2a3d4-2866-42c6-8797-f812b6a9c416" config-ref="HTTP_Listener_config" path="/system/accounts"/>
		<salesforce:query doc:name="SELECT Accounts" doc:id="418d987a-a966-4580-8164-e6b1a4d73430" config-ref="Salesforce_Config" >
			<salesforce:salesforce-query ><![CDATA[SELECT Id, Name, AccountNumber, Industry, Description, BillingStreet	
FROM Account
]]></salesforce:salesforce-query>
		</salesforce:query>
		<ee:transform doc:name="Transform Message" doc:id="88a4c0e0-c3bf-4958-a350-3ab2756ce264" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
payload]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
	<flow name="getContacts" doc:id="c5a93282-6a18-43d4-a866-6ed037b843b9" >
